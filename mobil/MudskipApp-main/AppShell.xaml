<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MudskipApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MudskipApp"
    Shell.FlyoutBehavior="Disabled"
    Title="MudskipApp">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage" />

    <ShellContent
        Title="Register"
        ContentTemplate="{DataTemplate local:RegisterPage}"
        Route="RegisterPage" />

    <ShellContent
        Title="Highscores"
        ContentTemplate="{DataTemplate local:HighscorePage}"
        Route="HighscorePage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    

</Shell>
